       <script>

     wss_i=0;
          wss_array=["Workshops","Lectures","Quiz","Contests"];


          function wss_increment()
          {
            wss_i++;
            wss.style.opacity = 0;
            if(wss_i > (wss_array.length -1))
            {
              wss_i=0;
            }
              setTimeout('wss_animation()',1000);
            
          }
           function wss_animation(){
                    wss.style.opacity=1;
                    wss.innerHTML=wss_array[wss_i];
                    setTimeout('wss_increment()',2000);
           }

           function startArticles(){
  var wScroll = $(window).scrollTop();

  if($('.articles').offset().top - $(window).height()/1.2 < wScroll) {
    $('.articles img').each(function(i){
      setTimeout(function(){
        $('.articles img').eq(i).addClass('is-visible');
      }, 100 * i);
    });
  }
}




  setInterval(function(){ tada();},2000);
  function tada(){
                var randnum=Math.floor(Math.random()* $("#a2 img").length) + 1;
                $(" #a2 img").eq(randnum).addClass("anime").siblings().removeClass("anime");
          }

           </script>

